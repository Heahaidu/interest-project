{
  "family": "event-service",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::852368830719:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::852368830719:role/eventServiceTaskRole",
  "containerDefinitions": [
    {
      "name": "event-service",
      "image": "REPLACED_BY_PIPELINE",
      "portMappings": [{ "containerPort": 8080, "protocol": "tcp" }],
      "essential": true
    }
  ]
}